{
  "name": "react-examples",
  "version": "1.0.0",
  "description": "React Examples, components and patterns ",
  "main": "index.js",
  "scripts": {
    "start": "node node_modules/webpack-dev-server/bin/webpack-dev-server --inline  --watch-content-base --mode development --open --config webpack.config.js --display-error-details",
    "build": "node ./node_modules/webpack/bin/webpack --mode production",
    "test": "node node_modules/jest/bin/jest.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daru23/react-examples.git"
  },
  "keywords": [
    "react",
    "javascript",
    "js",
    "patterns",
    "examples"
  ],
  "author": "Daniela Ruiz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/daru23/react-examples/issues"
  },
  "homepage": "https://github.com/daru23/react-examples#readme",
  "dependencies": {
    "bootstrap": "^4.1.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "collectCoverage": true,
    "setupFiles": [
      "jest-localstorage-mock",
      "./setupJest.js"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.3.0",
    "jest-fetch-mock": "^1.6.5",
    "jest-localstorage-mock": "^2.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.15.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  }
}
